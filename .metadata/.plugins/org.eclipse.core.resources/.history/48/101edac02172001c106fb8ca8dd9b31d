package day1;

import java.util.Scanner;

public class Table {
	
	public static void mulTable(int n)
	{
		for(int i=1;i<=n;i++)
		{
			for(int j=1;j<=n;j++)
			{
				System.out.print(" "+ i*j);
			}
			System.out.println();
		}
	}
	
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		mulTable(n);
		in.close();
	}
}
