package day1;

public class Fibonacci {
	
	public static int printFib(int n)
	{
		if(n==0)
			return 0;
		else if(n==1)
		{
			System.out.print(" "+1);
			return 1;
		}
		else
		{
			System.out.print(" "+n);
			return (printFib(n-1) + printFib(n-2));
		}
	}
	
	public static void main(String[] args) {
		
	}
}
