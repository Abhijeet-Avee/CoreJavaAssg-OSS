package day1;

import java.util.Scanner;

public class Table {
	
	public static void mulTable(int n)
	{
		System.out.print("*");
		for(int i=1;i<=n;i++)
		{
			System.out.print(" "+i);
			for(int j=0;j<=n;j++)
			{
				if(j==0)
					System.out.print(" "+ j);
				System.out.print(" "+ i*j);
			}
			System.out.println();
		}
	}
	
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		mulTable(n);
		in.close();
	}
}
