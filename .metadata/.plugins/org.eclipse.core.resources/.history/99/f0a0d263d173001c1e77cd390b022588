package com.day2.q2;

public class Book {
	private String bookTitle;
	private String author;
	private String ISBN;
	int numOfCopies;

	public Book(String title, String author, String ISBN, int numOfCopies) {
		this.bookTitle = title;
		this.author = author;
		this.ISBN = ISBN;
		this.numOfCopies = numOfCopies;
	}
}